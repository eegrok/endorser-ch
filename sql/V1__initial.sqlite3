
CREATE TABLE attendance (
       did CHARACTER(60), -- did:ethr are 52 chars
       eventRowId BIGINT
);

CREATE TABLE endorsement (
       did CHARACTER(60), -- did:ethr are 52 chars
       attendanceRowId BIGINT
);

CREATE TABLE event (
       name VARCHAR(255),
       startTime DATETIME
);

